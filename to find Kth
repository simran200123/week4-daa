#include <iostream>
#include <algorithm>
using namespace std;
int kthSmallest(int arr[],int n,int k) {
    sort(arr,arr+n);
    return arr[k-1];
}

int main() {
    int t;
    cin>>t;
    while (t--) {
        int n, k;
        cin >> n;
        int arr[n];
        for (int i=0;i<n;i++) 
        {
            cin>>arr[i];
        }
        cin>> k;
        cout<<kthSmallest(arr,n,k)<<endl;
    }
    return 0;
}


INPUT:-
3
10
123 656 54 765 344 514 765 34 765 234 
3
15
43 64 13 78 864 346 786 456 21 19 8 434 76 270 601
8



OUTPUT:-
123
